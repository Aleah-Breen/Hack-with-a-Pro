<button class = "subbot" onclick = myMap()> submit </button>




<div id="googleMap" style="width:100%;height:1000px;"></div>

<script>
  function myMap() {

// Diddy Riese location (for now)
const loc_Diddy_Riese = new google.maps.LatLng(34.063050026926234, -118.44685020759215);
var mapProp= {
  center: loc_Diddy_Riese,
  zoom: 50,
};

// Define a map variable
var map = new google.maps.Map(document.getElementById("googleMap"),mapProp);

// Define a Marker variable
var marker = new google.maps.Marker({position: loc_Diddy_Riese});

marker.setMap(map); 
}
</script>

<!-- Calls the Google Map API to display the map item-->
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDZn97dQcuirL7P1pkLLfRp9J7r56SPUys&callback=myMap"></script>

